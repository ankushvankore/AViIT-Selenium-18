package com.ArrayDemos;

public class D04StringDemo {

	public static void main(String[] args) {
		/*
		 * String - String is class in java
		 * It is non permeative data type in java
		 * It is 1D array of characters
		 * String is first class object in java
		 */
		String fName = new String("Dhanashri");
		String lName = "Thorat";
		
		System.out.println("First Name: " + fName);
		System.out.println("Last Name : " + lName);
		
		System.out.println("Index of D: " + fName.indexOf("D"));
	}

}
